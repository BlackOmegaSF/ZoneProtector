{
  "schemaVersion": 1,
  "id": "zone-protector",
  "version": "${version}",
  "name": "Zone Protector",
  "description": "A server-side mod to protect specified zones",
  "authors": [
    "Kian Leiner"
  ],
  "environment": "server",
  "entrypoints": {
    "server": [
      "com.kleinercode.fabric.zoneprotector.ZoneProtector"
    ]
  },
  "mixins": [
    "zone-protector.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}